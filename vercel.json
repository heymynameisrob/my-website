{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "portfolio.heymynameisrob.com"
        }
      ],
      "destination": "/portfolio/:path*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "portfolio.heymynameisrob.com"
        }
      ],
      "destination": "/portfolio/"
    }
  ]
}